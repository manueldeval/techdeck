section
  h1 Terminal
  .cols
    .col

      term( id="term1" 
            host-id="localhost"
            autostart=true 
            :font-size=16
            width='100%'
            height='400px'
            command="clear\\n")
    .col  
      ul
        li
          code(style="font-size:15px"  class="cmd" v-send-to-bus:term1.command) ls -al
        li
          code(style="font-size:15px"  class="cmd" v-send-to-bus:term1.command="'ls -al\\n'") push me
